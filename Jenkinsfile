    agent any
    stages { 
        stage('First') {
            steps {
                echo 'This is first message'
            }
        }
    }
    stages { 
        stage('Second') {
            steps {
                echo 'This is second message'
            }
        }
    }
     stages { 
        stage('Third') {
            steps {
                echo 'This is third message'
            }
        }
    }
     stages { 
        stage('Fourth') {
            steps {
                echo 'This is fourth message'
            }
        }
    }
}
